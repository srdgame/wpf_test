<Page x:Class="wpf_test.frames.UnitMgr"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:wpf_test.frames"
      xmlns:ctrls="clr-namespace:wpf_test.ctrls"
      mc:Ignorable="d" 
      d:DesignHeight="640" d:DesignWidth="800"
      Title="UnitMgr" Loaded="Page_Loaded">

    <Grid>
        <ctrls:PNTreeView x:Name="treeView" HorizontalAlignment="Left" Margin="10,10,0,10" Width="240" ClickAdd="treeView_ClickAdd" ClickEdit="treeView_ClickEdit" ClickDelete="treeView_ClickDelete" SelectedItemChanged="treeView_SelectedItemChanged"/>
        <Frame x:Name="frame" Margin="255,0,0,0"/>
    </Grid>
</Page>
