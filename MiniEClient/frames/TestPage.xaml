<Page x:Class="MiniEClient.frames.TestPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MiniEClient.frames"
      xmlns:ctrls="clr-namespace:MiniEClient.ctrls"
      mc:Ignorable="d" 
      d:DesignHeight="480" d:DesignWidth="640"
      Title="TestPage" Loaded="Page_Loaded">

    <Grid>
        <ctrls:PNTreeView x:Name="treeView" SelectedItemChanged="treeView_SelectedItemChanged" HorizontalAlignment="Left" Width="240" Margin="10,10,0,10" ClickAdd="treeView_ClickAdd" ClickEdit="treeView_ClickEdit" ClickDelete="treeView_ClickDelete"/>
        <Frame x:Name="frame" Margin="255,0,0,0"/>
    </Grid>
</Page>
