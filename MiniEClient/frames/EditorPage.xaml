<Page x:Class="MiniEClient.frames.EditorPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MiniEClient.frames"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="EditorPage" Loaded="Page_Loaded">

    <Grid>
        <Button x:Name="button" Content="Edit" HorizontalAlignment="Right" Margin="0,10,10,0" VerticalAlignment="Top" Width="75" Click="button_Click" Height="32" IsDefault="True"/>
        <Frame x:Name="frame" Margin="0,10,0,0" IsEnabled="False"/>

    </Grid>
</Page>
