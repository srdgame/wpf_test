<Window x:Class="MiniEClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MiniEClient"
        mc:Ignorable="d"
        Title="管理程序" 
        Height="720" Width="1024"
        MinHeight="720" MinWidth="1024"
        WindowStartupLocation="CenterScreen" Loaded="Window_Loaded">
    <Grid>
        <Frame x:Name="frame_left" HorizontalAlignment="Left" Width="120" Margin="0,64,0,0"/>
        <Frame x:Name="frame_main" Margin="120,64,0,0" NavigationUIVisibility="Hidden"/>
        <Frame x:Name="frame_top" Height="64" VerticalAlignment="Top"/>
    </Grid>
</Window>
